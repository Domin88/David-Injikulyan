#include <iostream>
using namespace std;
int main()
{
	unsigned n, k, sum = 0;
	cout << " nermuceq n";
	cin >> n;
	cout << " nermuceq k";
	cin >> k;
	for (int counter = 1; counter <= n; counter++)
		sum += pow(counter, (float)k); 
	cout << "arjeqy " << sum << endl;
	system("pause");
	return 0;
	

}
